            "exec":[
              "pm.test(\"CreateGame- Status code is 201\", function() {",
              "  pm.response.to.have.status(201);",
              "});",
              "",
              "pm.test(\"CreateGame- Verifies the schema is correct\", function() {",
              "  var characterSchema = {",
              "    \"type\": \"object\",",
              "    \"properties\": {",
              "      \"name\": {",
              "        \"type\": \"string\"",
              "      },",
              "      \"id\": {",
              "        \"type\": \"number\"",
              "      },",
              "      \"location\": {",
              "        \"type\": \"number\"",
              "      },",
              "      \"gameStateId\": {",
              "        \"type\": \"number\"",
              "      }",
              "    }",
              "  };",
              "",
              "  var gameStateSchema = {",
              "    \"type\": \"object\",",
              "    \"properties\": {",
              "      \"state\": {",
              "        \"type\": \"number\"",
              "      },",
              "      \"id\": {",
              "        \"type\": \"number\"",
              "      },",
              "      \"reason\": {",
              "        \"type\": \"null\"",
              "      },",
              "      \"gameID\": {",
              "        \"type\": \"number\"",
              "      },",
              "      \"characters\": {",
              "        \"type\": \"array\",",
              "        \"items\": characterSchema",
              "",
              "      }",
              "    }",
              "  };",
              "",
              "  var responseSchema = {",
              "    \"type\": \"object\",",
              "    \"properties\": {",
              "      \"id\": {",
              "        \"type\": \"number\"",
              "      },",
              "      \"startTime\": {",
              "        \"type\": \"string\"",
              "      },",
              "      \"totalTime\": {",
              "        \"type\": \"string\"",
              "      },",
              "      \"gameState\": gameStateSchema",
              "    }",
              "  };",
              "",
              "  var response = pm.response.json();",
              "  var schemaIsValid = tv4.validate(response, responseSchema);",
              "  pm.expect(schemaIsValid).to.be.true;",
              "});"
            ]
